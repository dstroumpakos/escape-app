# Unlocked – Escape Room Discovery App
# Copy this file to .env and fill in your values
# Variables prefixed with EXPO_PUBLIC_ are accessible in client code

# ── Convex ──
EXPO_PUBLIC_CONVEX_URL=https://your-deployment.convex.cloud

# ── Stripe / Payments ──
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxx

# ── Admin ──
# Must also be set in Convex Dashboard → Settings → Environment Variables as ADMIN_SECRET
EXPO_PUBLIC_ADMIN_SECRET=change-me-to-a-secure-secret

# ── Sentry (crash reporting) ──
EXPO_PUBLIC_SENTRY_DSN=https://xxxx@sentry.io/xxxx

# ── App Environment ──
EXPO_PUBLIC_APP_ENV=development
